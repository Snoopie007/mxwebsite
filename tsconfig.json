{
	"compilerOptions": {
	  "lib": ["dom", "dom.iterable", "esnext"],
	  "allowJs": true,
	  "skipLibCheck": true,
	  "strict": true,
	  "noEmit": true,
	  "esModuleInterop": true,
	  "module": "esnext",
	  "moduleResolution": "bundler",
	  "downlevelIteration": true,
	  "target": "ES2024",
	  "resolveJsonModule": true,
	  "isolatedModules": true,
	  "jsx": "preserve",
	  "incremental": true,
	  "plugins": [
		{
		  "name": "next"
		}
	  ],
	  "paths": {
		"@/*": ["./src/*"],
		"@public/*": ["./public/*"]
	  }
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "next.config.ts", "src/app/api/protected/loc/[id]/tax/settings"],
	"exclude": ["node_modules"]
  }
